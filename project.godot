[application]

config/name="project2v4"
run/main_scene="res://pre_title_menu/pre_title_menu.tscn"
config/icon="res://env/icon.png"

[audio]

default_bus_layout="res://env/default_audiobuslayout.tres"

[autoload]

Profile="*res://autoload/profile/profile.tscn"
SettingManager="*res://autoload/SettingManager.gd"
EffectManager="*res://autoload/effect_manager/effect_manager.tscn"
MusicManager="*res://autoload/music_manager/music_manager.tscn"

[debug]

